   "metadata": {},
   "outputs": [],
   "source": [
    "df = pd.read_excel(\"out.xlsx\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def save(x):\n",
    "    if x['A'] >= x['B']:\n",
    "        return x['A']\n",
    "    else:\n",
    "        return x['B']\n",
    "df['Prediction'] = df.apply(save, axis=1)"
